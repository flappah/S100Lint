	<xs:simpleType name="categoryOfRestrictedAreaType">
		<xs:annotation>
			<xs:documentation>Definition required</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="nature reserve">
				<xs:annotation>
					<xs:documentation>A tract of land or water managed so as to preserve it's flora, fauna, physical features, etc.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="bird sanctuary">
				<xs:annotation>
					<xs:documentation>A place where birds are bred and protected.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="game reserve">
				<xs:annotation>
					<xs:documentation>A place where wild animals or birds hunted for sport or food are kept undisturbed for private use.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="seal sanctuary">
				<xs:annotation>
					<xs:documentation>A place where seals are protected.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="historic wreck area">
				<xs:annotation>
					<xs:documentation>An area around certain wrecks of historical importance to protect the wrecks from unauthorized interference by diving, salvage or deposition (including anchoring).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
