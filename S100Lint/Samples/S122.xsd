	<xs:simpleType name="logicalConnectivesType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="logical conjunction">
				<xs:annotation>
					<xs:documentation>all the conditions described by the other attributes of the object, or sub-attributes of the same complex attribute, are true </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="logical disjunction ">
				<xs:annotation>
					<xs:documentation>at least one of the conditions described by the other attributes of the object, or sub-attributes of the same complex attributes, is true </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
